    "@fontsource/plus-jakarta-sans": "^4.5.9",
    "firebase": "^9.10.0",
    "framer-motion": "^6.5.1",
    "react": "^18.2.0",
    "react-chakra-pagination": "^1.0.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-table": "^7.8.0",
    "sweetalert2": "^11.4.33",
    "react-spinners": "^0.12.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.7.13",
    "@types/react": "^18.0.17",
