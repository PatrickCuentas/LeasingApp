{
  "name": "vite-react-chakra-starter",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
