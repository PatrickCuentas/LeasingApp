    "@types/react-helmet": "^6.1.5",
    "@types/react-table": "^7.7.12",
    "@vitejs/plugin-react": "^2.0.1",
    "commitlint": "^17.1.1",
    "eslint": "^8.23.0",
    "eslint-config-sznm": "^1.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.8.2",
    "vite": "^3.0.9"
